menu "Lamp configuration"

config WIFI_SSID
    string "WiFi SSID"
	default "myssid"
	help
		SSID (network name) for the lamp to connect to.

config WIFI_PASSWORD
    string "WiFi Password"
	default "mypwd"
	help
		WiFi password (WPA or WPA2) for the lamp to use.
		Can be left blank if the network has no security set.

endmenu